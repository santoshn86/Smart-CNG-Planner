<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:background="@drawable/g3"
    android:layout_height="match_parent"
    tools:context=".AdminPanel">


        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"

            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                >

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:onClick="open"
                    android:text="Check For Registered Users" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingLeft="2dp"
                    android:textAlignment="center"
                    android:layout_marginRight="5dp"
                    android:text="DATE"
                    android:background="@android:color/darker_gray"
                    />

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:hint="Enter"
                    android:id="@+id/date"

                    android:backgroundTint="@android:color/white"
                    android:background="@drawable/et_style"
                    />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingLeft="2dp"
                    android:textAlignment="center"
                    android:layout_marginRight="5dp"
                    android:text="Price"
                    android:background="@android:color/darker_gray"
                    />

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"

                    android:hint="Enter"
                    android:id="@+id/price"
                    android:backgroundTint="@android:color/white"
                    android:background="@drawable/et_style"
                    />

            </TableRow>




            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:background="@android:color/holo_green_dark"

                android:textAlignment="center"
                android:layout_marginTop="5dp"
                android:id="@+id/head"
                android:layout_marginBottom="10dp"
                android:textColor="@android:color/white"
                />

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:layout_marginRight="5dp"
                    android:textColor="@android:color/white"
                    android:text="Time Slots"
                    android:id="@+id/eer2"
                    android:background="@android:color/holo_blue_light"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textColor="@android:color/white"
                    android:id="@+id/eer3"
                    android:text="Slots Remain"
                    android:background="@android:color/holo_blue_light"
                    />

            </TableRow>



            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:layout_marginRight="5dp"
                    android:text="06:00 AM - 09:00 AM"
                    android:id="@+id/eer4"
                    android:background="@android:color/darker_gray"
                    />

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"

                    android:hint="Enter"
                    android:id="@+id/t1"
                    android:backgroundTint="@android:color/white"
                    android:background="@drawable/et_style"
                    />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:layout_marginRight="5dp"

                    android:text="09:00 AM - 12:00 PM"
                    android:id="@+id/eer5"
                    android:background="@android:color/darker_gray"
                    />

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"

                    android:backgroundTint="@android:color/white"
                    android:background="@drawable/et_style"
                    android:hint="Enter"            android:id="@+id/t2"
                    />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:id="@+id/eer6"
                    android:layout_marginRight="5dp"
                    android:text="12:00 PM - 03:00 PM"
                    android:background="@android:color/darker_gray"
                    />

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:backgroundTint="@android:color/white"
                    android:background="@drawable/et_style"

                    android:hint="Enter"            android:id="@+id/t3"
                    />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:id="@+id/eer7"
                    android:layout_marginRight="5dp"
                    android:text="03:00 PM - 06:00 PM"
                    android:background="@android:color/darker_gray"
                    />

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:hint="Enter"
                    android:id="@+id/t4"
                    android:backgroundTint="@android:color/white"
                    android:background="@drawable/et_style"                    />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:layout_marginRight="5dp"
                    android:id="@+id/eer8"
                    android:text="06:00 PM - 09:00 PM"
                    android:background="@android:color/darker_gray"
                    />

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:backgroundTint="@android:color/white"
                    android:background="@drawable/et_style"                    android:hint="Enter"
                    android:id="@+id/t5"
                    />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/eer9"
                    android:textAlignment="center"
                    android:layout_marginRight="5dp"
                    android:text="09:00 PM - 12:00 PM"
                    android:background="@android:color/darker_gray"
                    />

                <EditText
                    android:id="@+id/t6"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:backgroundTint="@android:color/white"
                    android:background="@drawable/et_style"
                    android:hint="Enter"
                    />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                >

                <Button
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_margin="5dp"
                    android:onClick="Save"
                    android:background="@drawable/et_style"
                    android:layout_height="wrap_content"
                    android:text="Save" />

                <Button
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_margin="5dp"
                    android:onClick="logout"
                    android:background="@drawable/et_style"
                    android:layout_height="wrap_content"
                    android:text="Logout" />
            </TableRow>

            </LinearLayout>
        </TableLayout>

</RelativeLayout>